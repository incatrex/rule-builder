  "widgets": {
    "text": {
      "type": "text",
      "valueSrc": "value",
      "factory": "materialui"
    },
    "number": {
      "type": "number",
      "valueSrc": "value",
      "factory": "materialui"
    },
    "date": {
      "type": "date",
      "valueSrc": "value",
      "factory": "materialui",
      "dateFormat": "YYYY-MM-DD",
      "valueFormat": "YYYY-MM-DD"
    },
    "boolean": {
      "type": "boolean",
      "valueSrc": "value",
      "factory": "materialui"
    },
    "select": {
      "type": "select",
      "valueSrc": "value",
      "factory": "materialui"
    },
    "multiselect": {
      "type": "multiselect",
      "valueSrc": "value",
      "factory": "materialui"
    }
  },
  "types": {
    "text": {
      "widgets": {
        "text": {
          "operators": [
            "equal",
            "not_equal",
            "like",
            "not_like",
            "starts_with",
            "ends_with",
            "is_empty",
            "is_not_empty"
          ]
        }
      }
    },
    "number": {
      "widgets": {
        "number": {
          "operators": [
            "equal",
            "not_equal",
            "less",
            "less_or_equal",
            "greater",
            "greater_or_equal",
            "is_empty",
            "is_not_empty"
          ]
        }
      }
    },
    "date": {
      "widgets": {
        "date": {
          "operators": [
            "equal",
            "not_equal",
            "less",
            "less_or_equal",
            "greater",
            "greater_or_equal",
            "is_empty",
            "is_not_empty"
          ]
        }
      }
    },
    "boolean": {
      "widgets": {
        "boolean": {
          "operators": [
            "equal",
            "not_equal"
          ]
        }
      }
    }
  },
  "settings": {
    "locale": {
      "moment": "en"
    },
    "maxNesting": 10,
    "canReorder": true,
    "canRegroup": true,
    "showNot": true,
    "showLabels": true,
    "setOpOnChangeField": ["keep", "default"],
    "clearValueOnChangeField": false,
    "clearValueOnChangeOp": false,
    "canLeaveEmptyGroup": true,
    "formatReverse": true,
    "formatField": "field",
    "valueSourcesInfo": {
      "value": {
        "label": "Value"
      },
      "field": {
        "label": "Field",
        "widget": "field"
      }
    },
    "canShortMongoQuery": true,
    "removeEmptyGroupsOnLoad": true,
    "removeEmptyRulesOnLoad": true,
    "removeIncompleteRulesOnLoad": false,
    "groupActionsPosition": "topRight",
    "setDefaultFieldAndOp": false
  }
}
