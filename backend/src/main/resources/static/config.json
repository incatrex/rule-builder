    "less": {
      "label": "<",
      "reversedOp": "greater_or_equal",
      "labelForFormat": "<",
      "cardinality": 1,
      "formatOp": "<",
      "jsonLogic": "<"
    },
    "less_or_equal": {
      "label": "<=",
      "reversedOp": "greater",
      "labelForFormat": "<=",
      "cardinality": 1,
      "formatOp": "<=",
      "jsonLogic": "<="
